{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "ESNext",
    "declaration": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
      "src",
  ],
  "exclude": [
      "node_modules",
  ]
}
